{
    "compilerOptions": {
        "module": "NodeNext",
        "esModuleInterop": true,
        "target": "ESNext",
        "moduleResolution": "NodeNext",
        "outDir": "dist",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "inlineSourceMap": true,
        "strict": true,
        "strictNullChecks": false,
        "paths": {
            "botinator": ["./node_modules/botinator/src"],
            "botinator/*": ["./node_modules/botinator/src/*"]
        }
    },
    "lib": [
        "ES2020"
    ]
}